

public class LinkedListPlayGround {

	private static class Node{
		int value;
		Node next;
		
		public Node(int value){
			this.value = value;
		}
	}
	
	public static void main(String[] args){
		Node n1 = new Node(1);
		Node n2 = new Node(2);
		Node n3 = new Node(3);
		n1.next = n2;
		n2.next = n3;
		
		test goes here
	}
	
	
	
}
